	tg_update(
		tg_bot_username: "some_nonexisting_bot",
		tg_update: { callback_query:, update_id: 261222353 })
	{ message: { message_id: 141856, reply_markup: { inline_keyboard: } } } := callback_query
	[{ callback_data: first }, *rest_buttons] := inline_keyboard
	[*middle_buttons, { callback_data: third }] := rest_buttons
	[{ callback_data: second }] := middle_buttons

